{
  "name": "tdd-node-back",
  "version": "1.0.0",
  "description": "Prject to improve knowledge about tdd and jst",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon index",
    "test": "cross-env NODE_ENV=test jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RolandoNajera/tdd-node-back.git"
  },
  "keywords": [
    "tdd",
    "jest",
    "node"
  ],
  "author": "Rolas Najera",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RolandoNajera/tdd-node-back/issues"
  },
  "homepage": "https://github.com/RolandoNajera/tdd-node-back#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "sequelize": "^6.7.0",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
