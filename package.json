{
  "name": "tdd-node-back",
  "version": "1.0.0",
  "description": "Prject to improve knowledge about tdd and jst",
  "main": "app.js",
  "scripts": {
    "start": "node app",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
